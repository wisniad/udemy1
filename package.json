{
  "name": "indecision-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://wisniad.github.io/udemy1",
  "dependencies": {
    "gh-pages": "^1.1.0",
    "node-sass-chokidar": "^1.2.2",
    "normalize-css": "^2.3.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-modal": "^3.3.2",
    "react-scripts": "1.1.1",
    "validator": "^9.4.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
